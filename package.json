{
  "name": "openlibrary",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "chart.js": "^3.5.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "tailwindcss": "^2.2.7",
    "vue-chartjs": "^3.5.1",
    "vue-template-compiler": "^2.6.11"
